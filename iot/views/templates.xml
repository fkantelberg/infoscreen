    <template id="assets_chart">
        <link rel="stylesheet" type="text/scss" href="/iot/static/lib/css/chart.min.css"/>
        <script type="text/javascript" src="/iot/static/lib/js/chart.min.js"></script>
    </template>

    <template id="menu">
        <t t-foreach="menu" t-as="item" class="item">
            <a t-attf-href="{{ item.url }}" t-attf-class="{{ item.icon_class or '' }}">
                <t t-esc="item.name" t-if="not item.icon_class"/>

                <span class="subicon" t-attf-name="{{ item.url }}"/>
            </a>
        </t>
    </template>

    <template id="submenu">
        <t t-call="iot.menu" t-if="submenu">
            <t t-set="menu" t-value="submenu"/>
        </t>

        <t t-if="pagination">
            <t t-if="lower is None">
                <t t-set="lower" t-value="1"/>
            </t>

            <a t-attf-href="{{ url }}?page={{ lower }}" t-if="page &gt; lower + 1">
                <span class="fa fa-angle-double-left"/>
            </a>

            <a t-attf-href="{{ url }}?page={{ page - 1 }}" t-if="page &gt; lower">
                <span class="fa fa-angle-left"/>
            </a>

            <a t-attf-href="{{ url }}?page={{ random.randint(lower, upper) }}" t-if="randomize">
                <span class="fa fa-random"/>
            </a>

            <a t-attf-href="{{ url }}?page={{ page + 1 }}" t-if="page &lt; upper">
                <span class="fa fa-angle-right"/>
            </a>

            <a t-attf-href="{{ url }}?page={{ upper }}" t-if="page &lt; upper - 1">
                <span class="fa fa-angle-double-right"/>
            </a>
        </t>
    </template>

    <template id="layout">&lt;!DOCTYPE html&gt;
        <html>
            <head>
                <meta charset="utf-8"/>
                <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/>

                <title t-esc="title or 'IoT'"/>

                <t t-call-assets="iot.assets" t-js="false"/>
                <t t-call-assets="iot.assets" t-css="false" defer_load="True"/>

                <t t-if="assets" t-raw="assets"/>
            </head>
            <body>
                <div id="menu" t-call="iot.menu"/>
                <div id="submenu" t-call="iot.submenu"/>

                <div id="wrapper" t-raw="0"/>
            </body>
        </html>
    </template>

    <template id="main_page">
        <t t-call="iot.layout"/>
    </template>
</odoo>
